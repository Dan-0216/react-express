{
  "name": "app-name-service",
  "version": "1.0.0",
  "description": "The scaffold of a react application project with express on the server side.",
  "main": "index.js",
  "scripts": {
  "start": "concurrently --kill-others \"gulp serve\" \"yarn client\"",
  "client": "node start-client.js",
  "start:debug": "cross-env DEBUG=open:* yarn start",
  "build": "gulp && yarn client:build",
  "client:build": "node start-client.js \"run build\" && shx cp -R client/build/ dist/client/ && shx rm -rf client/build",
  "lint": "esw *.js server config --color",
  "lint:watch": "yarn lint -- --watch",
  "precommit": "yarn lint && yarn test",
  "test": "cross-env NODE_ENV=local ./node_modules/.bin/mocha --ui bdd --reporter spec --colors --compilers js:babel-core/register server/tests --recursive",
  "test:watch": "yarn test -- --watch",
  "test:coverage": "cross-env NODE_ENV=local ./node_modules/.bin/istanbul cover _mocha -- --ui bdd --reporter spec --colors --compilers js:babel-core/register server/tests --recursive",
  "test:check-coverage": "yarn test:coverage && istanbul check-coverage",
  "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git@10.214.168.204:full-stack/react-express-scaffold.git"
  },
  "author": "xavier",
  "license": "ISC",
  "dependencies": {
    "async": "^2.6.0",
    "axios": ">=0.18.1",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "compression": "^1.6.2",
    "connect-multiparty": "^2.1.0",
    "connect-redis": "^3.3.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.3",
    "debug": "^2.6.8",
    "ejs": "^2.5.6",
    "express": "^4.15.3",
    "express-session": "^1.15.3",
    "express-validation": "^1.0.2",
    "express-winston": "^2.4.0",
    "formstream": "^1.1.0",
    "helmet": "^3.6.1",
    "http-status": "^1.0.1",
    "ioredis": "^3.1.1",
    "joi": "^10.5.2",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "method-override": "^2.3.9",
    "moment": "^2.18.1",
    "mongoose": "^4.10.7",
    "morgan": "^1.8.2",
    "nodemailer": "^4.4.0",
    "react-router-dom": "^4.2.2",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "urllib": "^2.22.0",
    "uuid": "^3.1.0",
    "winston": "^2.3.1",
    "winston-daily-rotate-file": "^1.4.6"
  },
  "devDependencies": {
   "babel-cli": "^6.24.1",
  "babel-core": "^6.24.1",
  "babel-plugin-add-module-exports": "^0.2.1",
  "babel-preset-es2015": "^6.24.1",
  "babel-preset-stage-2": "^6.24.1",
  "chai": "^4.0.1",
  "commitizen": "^2.9.6",
  "concurrently": "^3.5.0",
  "coveralls": "^2.13.1",
  "cross-env": "^5.0.0",
  "cz-conventional-changelog": "^2.0.0",
  "del": "^2.2.2",
  "eslint": "^3.19.0",
  "eslint-config-airbnb-base": "^11.2.0",
  "eslint-plugin-import": "^2.3.0",
  "eslint-watch": "^3.1.0",
  "gulp": "^3.9.1",
  "gulp-babel": "^6.1.2",
  "gulp-load-plugins": "^1.5.0",
  "gulp-newer": "^1.3.0",
  "gulp-nodemon": "^2.2.1",
  "gulp-rename": "^1.2.2",
  "gulp-sourcemaps": "^2.6.0",
  "gulp-util": "^3.0.8",
  "husky": "^0.13.4",
  "istanbul": "^0.4.5",
  "mocha": "^3.4.2",
  "run-sequence": "^1.2.2",
  "shx": "^0.2.2",
  "supertest": "^3.0.0",
  "supertest-as-promised": "^4.0.2",
  "validate-commit-msg": "^2.12.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "add-module-exports"
    ]
  }
}
